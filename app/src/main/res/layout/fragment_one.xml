<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:text="For 25m turbine"
        android:gravity="center_horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textColor="@color/authui_colorPrimaryDark"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:orientation="horizontal"
            android:background="@drawable/box_shape">
            <TextView
                android:id="@+id/numOfTurbineOneTV"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:textColor="@color/authui_colorPrimaryDark"/>
            <View
                android:layout_width="3dp"
                android:layout_height="wrap_content"
                android:background="#9b9292"/>

            <TextView
                android:id="@+id/oneTurbineOutputOneTV"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:textColor="@color/authui_colorPrimaryDark"/>
            <View
                android:layout_width="3dp"
                android:layout_height="wrap_content"
                android:background="#9b9292"/>
            <TextView
                android:id="@+id/totalTurbineOutputOneTV"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:textColor="@color/authui_colorPrimaryDark"/>


        </LinearLayout>

    <TextView
        android:text="For 50m turbine"
        android:gravity="center_horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textColor="@color/authui_colorPrimaryDark"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:orientation="horizontal"
            android:background="@drawable/box_shape"
            android:layout_marginTop="4dp">
            <TextView
                android:id="@+id/numOfTurbineTwoTV"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:textColor="@color/authui_colorPrimaryDark"/>
            <View
                android:layout_width="3dp"
                android:layout_height="wrap_content"
                android:background="#9b9292"/>
            <TextView
                android:id="@+id/oneTurbineOutputTwoTV"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:textColor="@color/authui_colorPrimaryDark"/>
            <View
                android:layout_width="3dp"
                android:layout_height="wrap_content"
                android:background="#9b9292"/>
            <TextView
                android:id="@+id/totalTurbineOutputTwoTV"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:textColor="@color/authui_colorPrimaryDark"/>

        </LinearLayout>
    <TextView
        android:text="For 100m turbine"
        android:gravity="center_horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textColor="@color/authui_colorPrimaryDark"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:orientation="horizontal"
            android:background="@drawable/box_shape"
            android:layout_marginTop="4dp">
            <TextView
                android:id="@+id/numOfTurbineThreeTV"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:textColor="@color/authui_colorPrimaryDark"/>
            <View
                android:layout_width="3dp"
                android:layout_height="wrap_content"
                android:background="#9b9292"/>
            <TextView
                android:id="@+id/oneTurbineOutputThreeTV"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:textColor="@color/authui_colorPrimaryDark"/>
            <View
                android:layout_width="3dp"
                android:layout_height="wrap_content"
                android:background="#9b9292"/>

            <TextView
                android:id="@+id/totalTurbineOutputThreeTV"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:textColor="@color/authui_colorPrimaryDark"/>

        </LinearLayout>




</LinearLayout>